@import '/styles/iconfont.wxss';
.icon-xuanze1{color: #f42424;}

.no-coupon {
  text-align: center;
	width: 100%;
	color: #666;
	padding:15rpx;
	font-size:25rpx;
  line-height: 100rpx;
}
.no-coupon text {
	display: inline;
	color: #2f54b7;
	background-color: transparent;
}

.coupons-container {
	height: calc(100% - 100rpx);
  border-top: 2rpx solid #f5f5f5;
}
.shop-item {
	display: flex;
	flex-direction: column;
}
.shop-item .title-shop-item {
	padding-left: 20rpx;
	height: 70rpx;
	background-color: #fff;
	font-size: 28rpx;
	line-height: 40rpx;
	color: #333;
	border-bottom: 2rpx solid #e2e2e2;
}
.shop-item .content-shop-item {
	min-height: 100rpx;
  width: 100%;
	text-align: center;
	background-color: #fff;
	padding: 0 20rpx;
}
.bonus-item {
	width: 100%;
	margin: 20rpx 0;
	background-color: #fff;
	box-shadow: 0 0 40rpx #ccc;
	transition: all .3s ease-out;
}
.bonus-item .checked {
	 box-shadow: 0 20rpx 40rpx #ccc;
}
.bonus-item.unavailable {
	 background-color: #f1f1f1;
	 filter: grayscale(100%);
 }
.top-bg-bonus-item {
	height: 22rpx;
	background: url('data:image/png;base64,') repeat-x 0 0;
	background-size: 20rpx 138rpx;
}
.content-bonus-item {
	display: flex;
	justify-content: space-around;
	width: 100%;
}
.check-bonus-item {
	display: flex;
	align-items: center;
	justify-content: center;
	width: 10%;
	text-align: center;
	position: relative;
}
.check-bonus-item icon{
	font-size: 32rpx;
}
.money-bonus-item {
	width: 90%;
	display: flex;
	flex-direction: column;
	justify-content: center;
  margin-bottom: 10rpx;
}
.use-money {
	width: 100%;
  height: 70rpx;
	color: #f42424;
	font-weight: bold;
	text-align: left;
}
.use-money text {
	color: #f42424;
}
.symbol-yuan {
	font-size: 32rpx;
}
.bonus-money {
	font-size: 56rpx;
}

.limit-money {
	width: 100%;
	font-size: 24rpx;
	color: #686868;
	text-align: left;
}
.limit-money .shop-name{
	color: #686868;
}
.limit-money .shop-name navigator{
  display: inline-block;
}