.distribution-message{
    width: 100%;
    display: flex;
    padding: 0 30rpx;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    line-height: 80rpx;
    position: relative;
    background-color: #fff;
    color: #333;
    font-size: 56rpx;
    overflow: hidden;
    border-bottom: 2rpx solid #e5e5e5;
}
.distribution-message .title{
    line-height: 100rpx;
    flex: 1;
    font-size: 28rpx;
}
.distribution-message icon{
    color: #999;
    font-size:24rpx;
    line-height: 100rpx;
}