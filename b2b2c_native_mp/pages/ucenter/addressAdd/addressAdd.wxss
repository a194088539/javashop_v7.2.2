.right-icon {
  color: #999;
  font-size: 24rpx;
  line-height: 24rpx;
  margin-top: 15rpx;
  margin-left: 10rpx;
  position: relative;
  display: inline-block;
  text-rendering: auto;
}
.right-icon .icon{
  width: 15rpx;
  height: 22rpx;
  vertical-align: middle;
}
.v_switch{
  text-align: right !important;
}
.big-btn {
  margin: 20rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
  color: #fff;
  background-color: #f42424;
  text-align: center;
  border-radius: 10rpx;
}
.valid .title {
  max-width: 180rpx;
}