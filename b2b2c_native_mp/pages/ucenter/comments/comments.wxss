page{
  width: 100%;
  height: 100%;
  background-color: #f7f7f7;
}
.comment-container {
  padding-bottom: 100rpx;
}
.cm-btn {
  position: fixed;
  bottom: 0;
  z-index: 999;
  border-radius: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
}
.cm-btn .text{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #f44;
  color: #fff;
  text-align: center;
  font-size: 28rpx;
}

.cm-item {
  width: 100%;
  background: white;
  margin-bottom: 30rpx;
}
.acm-nav{
  border-bottom: 2rpx solid #e4e2e2;
  padding-bottom:10rpx;
}
.acm-nav  .comments_score{
  padding: 0 20rpx;
  word-wrap: break-word;
}
  .cm-nav {
    display: flex;
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 2rpx solid #e4e2e2;
  }
  .cm-img {
    height: 80rpx;
    width: 80rpx;
    margin: 10rpx 10rpx 10rpx 20rpx;
  }
  .cm-grade {
    display: flex;
    width: calc(100% - 170rpx);
    margin-left: 60rpx;
  }
  .cm-grade .item-flower{
    display: flex;
    align-items: center;
    width: 160rpx;
    height: 100rpx;
    position: relative;
  }
  .cm-grade .item-flower input{
    position: absolute;
    width: 100%;
    height: 100%;
    opacity: 0;
  }
  .cm-grade .item-flower .i{
    display: inline-block;
    vertical-align: middle;
    width: 46rpx;
    height: 50rpx;
    margin-right: 10rpx;
  }
  .cm-grade .item-flower.praise .i{
    background: url("data:image/png;base64,") no-repeat 50%;
    background-size: 100%;
}
.cm-grade .item-flower.praise.active .i{
    background: url("data:image/png;base64,") no-repeat 50%;
    background-size: 100%;
}

.cm-grade .item-flower.average .i, .cm-grade .item-flower.bad-review .i{
    background: url("data:image/png;base64,") no-repeat 50%;
    background-size: 100%;
}
.cm-grade .item-flower.average.active .i,.cm-grade .item-flower.bad-review.active .i{
    background: url("data:image/png;base64,") no-repeat 50%;
    background-size: 100%;
}
.cm-con textarea{
  resize: none;
  height: 200rpx;
  width: 100%;
  padding: 20rpx 30rpx;
  background-color: #fff;
  border: none;
  box-sizing: border-box;
  overflow: hidden;
  font-size: 26rpx;
}
.shop-grade{
  background-color: #fff;
}
.shop-grade .shop-grade-nav{
  display: flex;
  align-items: center;
  height: 90rpx;
  line-height: 90rpx;
  position: relative;
  font-size: 28rpx;
}
.shop-grade .shop-grade-nav icon{
  font-size: 40rpx;
  margin: 0 10rpx;
}
.rating-star .logistics-star {
  height: 70rpx;
  line-height: 70rpx;
  clear: both;
}
.rating-star .logistics-star .text {
  float: left;
  font-size: 24rpx;
  margin-left: 6%;
  margin-right: 20rpx;
  color: #999;
}
.rating-star .logistics-star .star {
  float: left;
}
.rating-star .logistics-star .grade-text {
  float: left;
  font-size: 24rpx;
  color: #999;
  margin-left: 20rpx;
}
.comments-images-label {
  float: left;
}
.comments-images {
  width: 100rpx;
  height: 100rpx;
  margin-left: 10rpx;
  object-fit: contain;
}
.cm-photo {
  padding: 30rpx 20rpx;
  border-top: 2rpx solid #e4e2e2;
}
.cm-photos {
  display: flex;
  align-items: center;
}
.cm-photos .img-item {
  position: relative;
}
.cm-photos .img-item icon{
  position: absolute;
  top: -25rpx;
  right: 0;
  height:48rpx;
  font-size: 38rpx;
  color: #f42424;
  background-color: #fff;
  border-radius: 100%;
}
.cm-photos image {
  width: 100rpx;
  height: 100rpx;
  margin-right: 10rpx;
}
.cm-uploader icon{
  font-size: 50rpx;
}
.icon-xingxing{
  color: #f42424;
}
.icon-xingxing1,.icon-xingxing{
  font-size: 32rpx;
  margin-right: 5rpx;
}