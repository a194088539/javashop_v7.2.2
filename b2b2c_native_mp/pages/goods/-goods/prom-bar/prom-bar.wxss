.prom-bar-container{
	position: relative;
	display: flex;
	align-items: center;
	z-index: 7;
	height: 98rpx;
	color: #fff;
  background: url('data:image/png;base64,') no-repeat,-webkit-linear-gradient(top,#fc5997,#ef4747);
	background-size: 518rpx,100%;
}
.prom-bar-container .prom-price{
	margin: 0 15rpx 0 5rpx;
	font-weight: 700;
}
.prom-bar-container .prom-price text{
	font-size: 46rpx;
}
.prom-bar-container .msg{
	flex: 1;
  position: relative;
}
.prom-bar-container .msg .icon{
	position: relative;
	display: inline-block;
	margin: -4rpx 6rpx 0 0;
	padding: 0 6rpx 0 36rpx;
	height: 30rpx;
	vertical-align: middle;
	line-height: 30rpx;
	font-size: 20rpx;
	color: #fff;
}
.prom-price,.prom-price text{
	color: #fff;
}
.prom-bar-container .msg .old-price{
	text-decoration:line-through;
	color: #fff;
}
.prom-bar-container .msg .icon.seckill::before {
	background: #fff url(data:image/png;base64,) no-repeat 50%;
  background-size: 24rpx;
}
.prom-bar-container .msg .icon.groupbuy::before {
  background: #fff url(data:image/png;base64,) no-repeat 50%;
  background-size: 24rpx;
}
.prom-bar-container .msg .icon::before {
    content: "";
    position: absolute;
    left: 0;
    top: 0;
    width: 30rpx;
    height: 30rpx;
    border-top-left-radius: 2rpx;
    border-bottom-left-radius: 2rpx;
}
.prom-bar-container .msg .icon::after {
	content: "";
	display: block;
	border: 2rpx solid #fff;
	position: absolute;
	top: 0;
	left: 0;
	pointer-events: none;
	bottom: 0;
	right: 0;
	border-radius: 4rpx;
}
.prom-bar-container .count-down{
	position: absolute;
	right: 0;
	top: 0;
	min-width: 200rpx;
	height: 100%;
	text-align: center;
	background-color: #f8f8f8;
}
.prom-bar-container .count-down .count-down-times{
	font-size: 28rpx;
	color: #f42424;
}
.prom-bar-container .count-down .count-down-times .time{
	display: inline-block;
  margin: 0 2rpx;
  width: 46rpx;
  height: 42rpx;
  line-height: 42rpx;
  font-weight: 700;
  color: #fff;
  background-color: #f42424;
}
.prom-bar-container .count-down .count-down-times .day {
  font-size: 24rpx;
  color:#f42424;
  margin: 0 10rpx;
}
.prom-bar-container .count-down .count-down-text{
  margin: 10rpx 0 4rpx;
	font-size: 20rpx;
	color: #666;
}