.van-cell {
	width: 100%;
	height: 94rpx;
	display: flex;
	padding: 20rpx 0 20rpx 30rpx;
	box-sizing: border-box;
	line-height: 48rpx;
	position: relative;
	background-color: #fff;
	color: #333;
	font-size: 28rpx;
	overflow: hidden;
	border-bottom: 2rpx solid #f3f3f3;
}
.cell_title, .cell_value {
	flex: 1;
}
.cell_value{
	overflow: hidden;
	text-align: right;
	position: relative;
	vertical-align: middle;
}
.van-cell .i{
	float:right;
	width:45rpx;
	height:45rpx;
	margin-right:16rpx;
}
.rate{
  color: #f42424;
}
.some-comments {
  border: 2rpx solid #f5f5f5;
}
.assemble-swipe{
  width: 100%;
  height: 240rpx;
}
.assemble-slide{
  padding: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.assemble-slide .assemble-person{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.assemble-slide .assemble-person image{
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  margin: 0 10rpx;
  object-fit: cover;
}
.assemble-slide .assemble-info{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}
.assemble-slide .assemble-info .btn{
  margin-left: 20rpx;
  height: 55rpx;
  line-height: 55rpx;
  padding: 0 16rpx;
  min-width: 120rpx;
  text-align: center;
  background-color: #f42424;
  color: #fff;
  border-radius: 8rpx;
}
.comments-list{
  border-top: 2rpx solid #f5f5f5;
  height: calc(100% - 100rpx);
}