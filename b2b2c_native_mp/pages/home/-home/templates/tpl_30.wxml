<template name="tpl_30">
  <view class="floor-layout tpl-30">
    <view class="layout-main">
      <view class="layout-item" bindtap="blockHref" data-block="{{item.blockList[0]}}">
        <image lazy-load wx:if="{{item.blockList[0].block_type === 'IMAGE'}}" src="{{item.blockList[0].block_value}}"></image>
      </view>
    </view>
  </view>
</template>