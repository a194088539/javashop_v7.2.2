.wrapAnimate{animation: wrapAnimate 0.5s ease-in-out forwards}
@keyframes wrapAnimate{
  0%{}
  100%{background:rgba(0,0,0,0.35);}
}
.wrapAnimateOut{animation: wrapAnimateOut 0.4s ease-in-out forwards}
@keyframes wrapAnimateOut{
  0%{background:rgba(0,0,0,0.35);}
  100%{background:rgba(0,0,0,0);}
}
.frameAnimate{animation: frameAnimate 0.5s ease forwards;}
@keyframes frameAnimate{
  0%{}
  100%{opacity: 1;top:0;}
}
.frameAnimateOut{animation: frameAnimateOut 0.4s ease forwards;}
@keyframes frameAnimateOut{
  0%{opacity: 1;top:0;}
  100%{opacity: 0;top:100%;}
}
.frame-wrapper{position: fixed;height:100%;width:100%;z-index: 12;top: 50%;}
.frame{background: #fff;position: absolute;bottom: 0;width: 100%;height:70%;border-top-left-radius: 20rpx;border-top-right-radius: 20rpx;z-index: 13;}
.title-wrapper{justify-content: space-between;padding: 20rpx;}
.title-wrapper .title{font-size: 30rpx;color: #4a4a4a;}
.title-wrapper image{width:45rpx;height: 45rpx;}
.flex{display: flex;align-items: center;}
.wrap{position: fixed;z-index: 11;top: 0;left: 0;right: 0;bottom: 0;}
