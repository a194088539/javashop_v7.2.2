@import '../../styles/iconfont.wxss';
.movable-area{
  pointer-events:none;
  z-index: 100;
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.movable-view{ 
  pointer-events:auto;
  height: 76rpx;
  width: 76rpx;
  background: #fff;
  border-radius: 100%;
  text-align: center;
  box-shadow: 0 0 20rpx #ccc;
}
.movable-view .icon {
  line-height:76rpx;
  font-size:50rpx;
  color:#ccc;
}